  <div class="container-fluid" id="signin">
    <div class="row">
      <div class="form-group">
        <div class="col-sm-offset-5 col-sm-7">
        <h1>Login</h1>
        </div>
        <%= form_for(resource, :html => {class: "form-horizontal", id: "signin"}, url: session_path(resource_name)) do |f| %>

      <div class="form-group">
      <%= f.label :email, nil, :class => "col-sm-2 control-label" %>
      <%= f.email_field :email, autofocus: true, :class => "col-sm-8" %>
      </div>

      <div class="form-group">
      <%= f.label :password, nil, :class => "col-sm-2 control-label" %>
      <%= f.password_field :password, autocomplete: "off", :class => "col-sm-8" %>
      </div>

      <div class="form-group">
      <% if devise_mapping.rememberable? %>
      <div class="col-sm-offset-2 col-sm-10" id="remember">
        <div class="checkbox" id="check">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
        <% end %>
        </div>
      </div>
      </div>

      <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10" id="log" >
        <%= f.submit  "Log in", :class => "btn btn-default"%>
        <% end %>
        </div>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-sm-offset-2 col-sm-10" id="links">
      <%= render "devise/shared/links" %>
    </div>
  </div>
</div>